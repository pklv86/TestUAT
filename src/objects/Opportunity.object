var execRecords = execQuotes.getArray(&quot;records&quot;);
if(execRecords.length &lt;= 0) {
	var qualifiedQuotes = sforce.connection.query(&quot;SELECT ID from Retail_Quote__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; AND Request_Status__c = &apos;Pricing Complete&apos;&quot;); 

