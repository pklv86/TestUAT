        <fullName>Price_Request_Refresh</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Price Request/Refresh</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var tempdate=new Date(&apos;{!Opportunity.Pricing_Due_Date__c}&apos;); 
var currDate = new Date(&apos;{!TODAY()}&apos;);
if(tempdate &lt; currDate){ 
alert(&apos;{!$Label.Pricing_Due_Date}&apos;); 
} 
else 
{ 

var  message = sforce.apex.execute(&quot;RequestPricingInfo&quot;,&quot;requestPriceStatus&quot;,{oppId:&quot;{!Opportunity.Id}&quot;}).toString(); 

var result = message.split(&apos;:&apos;); 
if (result[0] != &apos;&apos;) { 
alert(result[0]); 
} 
if (result[1] == &apos;YES&apos;) { 
window.location.reload(true); 
} 
} 

