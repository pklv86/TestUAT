    <validationRules>
        <fullName>Closed_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL( StageName , &apos;Closed Lost&apos;) &amp;&amp; ISBLANK( Closed_Win_Loss_Comments__c ) &amp;&amp; ! ISCHANGED( Acct_Annual_Usage_Sum__c )&amp;&amp; ! ISCHANGED(  Acct_Annual_Usage_Sum_MHw__c) &amp;&amp; ! ISCHANGED( LDC_Individual_Info__c), true, false)</errorConditionFormula>
        <errorDisplayField>Closed_Win_Loss_Comments__c</errorDisplayField>
        <errorMessage>Enter the Close Win/Loss comments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pricing_Due_date_greater_than_today</fullName>
        <active>false</active>
        <description>This validation will not allow user to enter a date that is less than today</description>
        <errorConditionFormula>AND(! ISCHANGED( Acct_Annual_Usage_Sum__c ) ,! ISCHANGED(  Acct_Annual_Usage_Sum_MHw__c ) ,Pricing_Due_Date__c &lt; TODAY() ,     ! ISCHANGED( LDC_Individual_Info__c ) )</errorConditionFormula>
        <errorMessage>Pricing due date must be equal or greater than today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Substage_Required_For_Closed_Lost</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISPICKVAL( StageName , &apos;Closed Lost&apos;) &amp;&amp; ISPICKVAL( Sub_stage__c , &apos;&apos;) &amp;&amp; ! ISCHANGED( LDC_Individual_Info__c ), true,false)</errorConditionFormula>
        <errorMessage>Please Select Sub-Stage for Closed Lost</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Executable_Price</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Executable Price</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

var execQuotes = sforce.connection.query(&quot;SELECT ID,Request_Status__c from Retail_Quote__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; AND recordType.name=&apos;Executable&apos;&quot;); 

var execRecords = execQuotes.getArray(&quot;records&quot;); 
var exeWaitingRec = sforce.connection.query(&quot;SELECT ID,Request_Status__c from Retail_Quote__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; AND Request_Status__c =&apos;Waiting on Load Data&apos;&quot;); 

var exeWaitingLst = exeWaitingRec.getArray(&quot;records&quot;); 

var retPriceQuotes = sforce.connection.query(&quot;SELECT ID,Request_Status__c from Retail_Quote__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; AND recordType.name=&apos;Executable&apos; and Request_Status__c =&apos;Pricing Complete&apos;&quot;); 
var retPriceQtLst = retPriceQuotes.getArray(&quot;records&quot;);

var retQuotes = sforce.connection.query(&quot;SELECT ID,Request_Status__c from Retail_Quote__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos;&quot;); 
var retQtLst = retQuotes.getArray(&quot;records&quot;);

var tempdate=new Date(&apos;{!Opportunity.Pricing_Due_Date__c}&apos;); 
var currDate = new Date(&apos;{!TODAY()}&apos;);
if(tempdate &lt; currDate){ 
alert(&apos;{!$Label.Pricing_Due_Date}&apos;); 
} 
else if(retPriceQtLst.length &gt; 0 &amp;&amp; retPriceQtLst.length == retQtLst.length)
{
  alert(&apos;{!$Label.Executable_Quote_Generated}&apos;);
}
else if(exeWaitingLst.length &gt; 0 ) 
{ 
alert(&apos;{!$Label.Waiting_on_Pricing_Response}&apos;); 
} 
else if(execRecords.length &lt;= 0) { 
var qualifiedQuotes = sforce.connection.query(&quot;SELECT ID from Retail_Quote__c WHERE Opportunity__c = &apos;{!Opportunity.Id}&apos; AND Request_Status__c = &apos;Pricing Complete&apos;&quot;); 

var records = qualifiedQuotes.getArray(&quot;records&quot;); 

if(records.length &lt;= 0) { 
alert(&apos;{!$Label.No_Quotes_available_for_Executable_Price}&apos;); 
} 
else{ 
window.open(&apos;/apex/GetExecutablePrice?id={!Opportunity.Id}&apos;,&apos;_parent&apos;, &apos;toolbar=yes, scrollbars=yes&apos;); 
} 

} 
else{ 
window.open(&apos;/apex/GetExecutablePrice?id={!Opportunity.Id}&apos;,&apos;_parent&apos;, &apos;toolbar=yes, scrollbars=yes&apos;); 
}

/*
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/apex.js&quot;)} 

