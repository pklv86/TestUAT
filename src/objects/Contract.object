        <fullName>Mark_for_Enrollment</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Mark for Enrollment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 

if(&apos;{!Contract.Status}&apos;!=&apos;Activated&apos;){
   alert(&apos;{!$Label.Contract_Not_Active_MSG}&apos;);

}
else if(&apos;{!Contract.Enrollment_Stage__c}&apos;==&apos;Ready to Enroll&apos;){
     
    alert(&apos;{!$Label.Contract_RE_Mark_Enrollment_MSG}&apos;);
}
else if(&apos;{!Contract.Supplier__c}&apos; == &apos;&apos;){   
   alert(&apos;{!$Label.Contract_Supplier}&apos;);
}

else if(&apos;{!Contract.Customer_Type__c}&apos;==&apos;Muni Agg Customer&apos; ){
   
     if(new Date(&apos;{!Contract.Opt_Out_End_Date__c}&apos;)&gt;Date.now())
      alert(&apos;{!$Label.Muni_Agg_Enrollment_Opt_Out_MSG}&apos;);
      else if(&apos;{!Contract.Opt_Out_End_Date__c}&apos;==&apos;&apos;)
        alert(&apos;{!$Label.Opt_Out_End_Date_MSG}&apos;);
     else
       markEnrollment();
    
}
else if(&apos;{!Contract.Customer_Type__c}&apos;==&apos;C&amp;I Customer&apos;){
    markEnrollment();
}
else{
     alert(&apos;{!$Label.Invalid_Contract_To_Enroll}&apos;);
}

function markEnrollment(){
        var Cntrct = new sforce.SObject(&quot;Contract&quot;);
        Cntrct.Id = &apos;{!Contract.Id}&apos;;
        Cntrct.Enrollment_Stage__c=&apos;Ready to Enroll&apos;;
        
